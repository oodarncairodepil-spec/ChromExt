# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL
PLASMO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key
PLASMO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Your Supabase service role key (optional, for admin operations)
PLASMO_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# RajaOngkir API Key (for shipping calculations)
PLASMO_PUBLIC_RAJAONGKIR_API_KEY=your-rajaongkir-api-key-here
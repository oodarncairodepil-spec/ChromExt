{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    }
  },
  "version": "0.0.1",
  "author": "Your Name",
  "name": "DEV | Chrome Side Panel Extension",
  "description": "A Chrome MV3 extension with side panel React app",
  "background": {
    "service_worker": "./static/background/index.ts"
  },
  "side_panel": {
    "default_path": "./sidepanel.html"
  },
  "permissions": [
    "sidePanel",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
